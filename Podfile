  pod 'RealmSwift', git: 'https://github.com/realm/realm-cocoa.git', branch: 'master', :submodules => true

  
  target 'ClexiTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ClexiUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
